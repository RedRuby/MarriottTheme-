{% endif %}

<script>
  var cat_1,cat2;
  $(document).ready(function(){
    $("#logo-grid1 a").on('click',function(e){
    	e.preventDefault();
      	cat_1 = $(this).attr('href');
      	$("#logo-grid1 a img").css('opacity','0.3');
      	$(this).find('img').css('opacity','1');
      	$("#featured-shop-grid").show();
        $('body, html').delay(600).animate({scrollTop: $("#featured-shop-grid").offset().top-80});
      	return false;
    });
    $("#featured-shop-grid .lp-feature").on('click',function(e){
    	e.preventDefault();
      	cat_2 = $(this).attr('href');
      	if(cat_1)
          location.assign(cat_1+cat_2);
      	else
          scroll2Cat1();
      	return false;
    });
  });
  
  function scroll2Cat1(){
  	$('body, html').delay(100).animate({scrollTop: $("#logo-grid1").offset().top-300});
  }
</script>
